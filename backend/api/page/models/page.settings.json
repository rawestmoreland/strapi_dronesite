{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "name": "Page",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "shortName": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "metadata": {
      "type": "component",
      "repeatable": false,
      "component": "meta.metadata",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "blocks.hero",
        "blocks.feature-columns-group",
        "blocks.feature-rows-group"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "published",
        "draft"
      ],
      "default": "draft",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "string",
      "regex": "^$|^[a-zA-Z/-]+$",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "default": " "
    }
  }
}
